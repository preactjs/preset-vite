{
	"compilerOptions": {
		"target": "ES2019",
		"strict": true,
		"strictNullChecks": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"module": "node20",
		"moduleResolution": "node16",
		"noEmit": true,
		"allowJs": true,
		"checkJs": true,
		"skipLibCheck": false,
		"maxNodeModuleJsDepth": 0
	},
	"include": ["src/**/*"],
}
